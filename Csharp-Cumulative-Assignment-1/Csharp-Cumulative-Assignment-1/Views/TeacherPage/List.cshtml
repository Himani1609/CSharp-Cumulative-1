@model IEnumerable<Csharp_Cumulative_Assignment_1.Models.Teacher>


<!-- Displaying all the teachers on the web page from where you can select any teacher  -->

@foreach (Csharp_Cumulative_Assignment_1.Models.Teacher Teacher in Model)
{
    <div>
        <a href="/TeacherPage/Show/@Teacher.TeacherId">@Teacher.TeacherFName @Teacher.TeacherLName</a>
    </div>
}
